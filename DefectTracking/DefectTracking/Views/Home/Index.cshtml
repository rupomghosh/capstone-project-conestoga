        <tr>
            <th class="text-md-center">
                <h1 class="text-white">Forms</h1>
            </th>
            <th class="text-md-center">
                <h1 class="text-white">Records</h1>
            </th>
            <th class="text-md-center">
                <h1 class="text-white">Reports</h1>
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th class="text-md-center">
                <p></p>
                <a class="btn btn-outline-white text-white" asp-area="" asp-controller="Forms" asp-action="ManufacturingForm"><h1>Manufacturing Defect</h1></a>
                <p></p>
            </th>
            <th class="text-md-center">
                <p></p>
                <a class="btn btn-outline-white text-white" asp-area="" asp-controller="Forms" asp-action="ManufacturingIndex"><h1>Manufacturing Defect</h1></a>
                <p></p>
            </th>
            <th class="text-md-center">
                <p></p>
                <a class="btn btn-outline-white text-white" asp-area="" asp-controller="Report" asp-action="ManufacturingReportSummary"><h1>Manufacturing Defect</h1></a>
                <p></p>
            </th>
        </tr>
        <tr>
            <th class="text-md-center">
                <p></p>
                <a class="btn btn-outline-white text-white" asp-area="" asp-controller="Forms" asp-action="DeliveryForm"><h1>Delivery Defect</h1></a>
